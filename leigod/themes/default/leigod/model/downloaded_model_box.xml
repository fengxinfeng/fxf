<?xml version="1.0" encoding="UTF-8"?>
<Window >
	
	<ListContainerElement name="download_item" width="796" height="62" bkcolor="" visible="true"   borderround="2,2,2,2">	
    <Progress class="progress_grey" name="progress"   value="100" margin="10"   borderround="2,2,2,2"/>
    <HBox name="download information" height="62" >
      <Label   width="30" height="30" margin="15,0,0,0" valign="center" bkcolor="" bkimage="label/download_file.png"/>
      <Label   width="125"  margin="14,0,0,0" valign="center" text="file name......................................" font="system_12"  bkcolor="" normaltextcolor="white"/>
      <Label   width="90"  margin="29,0,0,0" valign="center" text="135M/2.6G" font="system_12"  bkcolor="" normaltextcolor="white1"/>
      <Label   width="8" height="12" margin="3,0,0,0" valign="center" bkcolor="" bkimage="label/fast.png"/>
      <Label   width="66"  margin="8,0,0,0" valign="center" text="77.3M/s" font="system_12"  bkcolor="" normaltextcolor="white1"/>
      <Label   width="90"  margin="11,0,0,0" valign="center" text="下载已完成" font="system_12"  bkcolor="" normaltextcolor="white1"/>
      <Control />
      <Button  name="openfile" width="16" height="16" margin="0,0,18,0" valign="center" bkcolor=""
                normalimage="file='btn/openfile.png' source='0,0,16,16'" 
								hotimage="file='btn/openfile.png' source='16,0,32,16'" 
								pushedimage="file='btn/openfile.png' source='32,0,48,16'" 
								disabledimage="file='btn/pause.png' source='42,0,64,16'"/>
      <Button  name="openfolder" width="16" height="16" margin="0,0,18,0" valign="center" bkcolor=""                
                normalimage="file='btn/openfolder.png' source='0,0,16,16'"
								hotimage="file='btn/openfolder.png' source='16,0,32,16'"
								pushedimage="file='btn/openfolder.png' source='32,0,48,16'"
								disabledimage="file='btn/openfolder.png' source='42,0,64,16'"/>
      <Button  name="remove" width="16" height="16" margin="0,0,27,0" valign="center" bkcolor=""                
                normalimage="file='btn/del.png' source='0,0,16,16'" 
								hotimage="file='btn/del.png' source='16,0,32,16'" 
								pushedimage="file='btn/del.png' source='32,0,48,16'" 
								disabledimage="file='btn/del.png' source='42,0,64,16'"/>
    </HBox>
  </ListContainerElement>
	
</Window> 